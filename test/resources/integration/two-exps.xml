					</tuning>
				</app>
				<topology nc="12" cpv="12" />
				<mapping idf="0" pstrat="BAL_SET" />
				<technology network="virtio" disk="virtio" />
				<tuning>
					<hugepages>True</hugepages>
					<cpuTopology type="DEFAULT" guestNuma="False" />
