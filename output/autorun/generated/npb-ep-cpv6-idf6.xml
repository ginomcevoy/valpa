<experiments>
	<experiment name="nc12-cpv6-idf6-psBAL_ONE-ppTrue" trials="1">
		<clusters>
			<cluster>
				<app name="npb-ep" runs="10">
				    <args>12 C</args>
					<tuning>
						<procpin>True</procpin>
					</tuning>
				</app>
				<topology nc="12" cpv="6" />
				<mapping idf="6" pstrat="BAL_ONE" />
