<?xml version="1.0"?>
<experiments>
        <experiment name="cpv2" trials="1">
                <clusters>
                        <cluster>
                                <app name="parpac" runs="2">
                                        <tuning>
                                                <procpin>True</procpin>
                                        </tuning>
                                </app>
                                <topology nc="8" cpv="2"/>
                                <mapping idf="0" pstrat="BAL_SET" />
                                <technology network="virtio" disk="virtio"/>
                                <tuning>
                                        <hugepages>True</hugepages>
                                        <cpuTopology type="DEFAULT" guestNuma="False"/>
                                        <numatune>False</numatune>
                                        <ballooning>False</ballooning>
                                </tuning>
                        </cluster>
                </clusters>
        </experiment>
        <experiment name="cpv4" trials="1">
                <clusters>
                        <cluster>
                                <app name="parpac" runs="3">
                                        <tuning>
                                                <procpin>True</procpin>
                                        </tuning>
                                </app>
                                <topology nc="8" cpv="4"/>
                                <mapping idf="0" pstrat="SPLIT" />
                                <technology network="virtio" disk="ide"/>
                                <tuning>
                                        <hugepages>True</hugepages>
                                        <cpuTopology type="DEFAULT" guestNuma="False"/>
                                        <numatune>False</numatune>
                                        <ballooning>False</ballooning>
                                </tuning>
                        </cluster>
                </clusters>
        </experiment>
</experiments>
